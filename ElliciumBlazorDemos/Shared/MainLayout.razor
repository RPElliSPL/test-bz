<ElliciumComponents />

<ElliciumLayout Style="grid-template-columns: auto 1fr auto; grid-template-areas: 'rz-header rz-header rz-header' 'rz-sidebar rz-body rz-config-sidebar'">
<ElliciumHeader>
    <ChildContent>
        <ElliciumRow AlignItems="AlignItems.Center" JustifyContent="JustifyContent.Start" Gap="0px">
            <ElliciumColumn Size="5">
                <ElliciumStack Orientation="Orientation.Horizontal" AlignItems="AlignItems.Center">
                    <ElliciumSidebarToggle Click="@(() => sidebarExpanded = !sidebarExpanded)" />
                    <span style="overflow: hidden; white-space: nowrap; text-overflow: ellipsis;">Ellicium Blazor Components</span>
                </ElliciumStack>
            </ElliciumColumn>
            <ElliciumColumn Size="7">
                <ElliciumStack  Orientation="Orientation.Horizontal" AlignItems="AlignItems.Center" JustifyContent="JustifyContent.End">
                    <div Class="rz-display-none rz-display-sm-inline-flex align-items-center">
                        <a href="/docs/" title="Ellicium Blazor Components Documentation" Class="rz-mx-2 rz-mx-lg-4" target="_blank">Documentation</a>
                        <a href="https://www.Ellicium.com/" title="Create .NET Core web applications fast & easy" Class="rz-text-nowrap rz-mx-2 rz-mx-lg-4" target="_blank">About Ellicium</a>
                        <a href="https://github.com/Elliciumhq/Ellicium-blazor" title="Star Ellicium Blazor Components on GitHub" Class="rz-text-nowrap rz-mx-2 rz-mx-lg-4" target="_blank">GitHub</a>
                    </div>
                    <ElliciumSidebarToggle Icon="settings" Click="@(() => configSidebarExpanded = !configSidebarExpanded)" Class="rz-m-0" />
                </ElliciumStack>
            </ElliciumColumn>
        </ElliciumRow>
    </ChildContent>
</ElliciumHeader>
